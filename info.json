{
    "manufacturer": "Keychron",
    "keyboard_name": "Keychron Q1",
    "url": "https://github.com/Keychron",
    "maintainer": "keychron-contrib",
    "processor": "STM32L432",
    "bootloader": "stm32-dfu",
    "usb": {
        "vid": "0x3434"
    },
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "rgb_matrix": true,
        "dip_switch": true
    },
    "eeprom": {
        "wear_leveling": {
            "backing_size": 4096
        }
    },
    "dip_switch": {
        "matrix_grid": [ [5, 4] ]
    },
    "matrix_pins": {
        "cols": ["C14", "C15", "A0", "A1", "A2", "A3", "A4", "A5", null, null, null, null, null, null, null, null],
        "rows": ["B5", "B4", "B3", "A15", "A14", "A13"],
        "custom": true,
        "custom_lite": true
    },
    "diode_direction": "ROW2COL",
    "rgb_matrix": {
        "driver": "snled27351",
        "sleep": true
    }
}
